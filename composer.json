{
    "name": "php-panel/ladmin-ext-backup",
    "description": "Backup extension for laravel-admin",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "backup"
    ],
    "homepage": "https://github.com/php-panel/ladmin-ext-backup",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "leeqvip",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "php-panel/laravel-admin": "^3.0",
        "spatie/laravel-backup": "^9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "laravel/laravel": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Ladmin\\Backup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladmin\\Backup\\BackupServiceProvider"
            ]
        }
    }
}